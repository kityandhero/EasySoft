{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/{url}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "ServiceName": "testService",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "host.docker.internal",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}