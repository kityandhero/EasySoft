{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "oneservice.local.com",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/one/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 3,
        "DurationOfBreak": 10000,
        "TimeoutValue": 5000
      }
    }
  ],
  "GlobalConfiguration": {
  }
}